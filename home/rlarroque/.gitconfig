[alias]
        tree = log --graph --decorate --pretty=oneline --abbrev-commit --all
        st = status
        co = checkout
        undo = reset --soft HEAD^

[color]
        branch = auto
        diff = auto
        interactive = auto
        status = auto

[push]
        default = simple

[merge]
        tool = meld
